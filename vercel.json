{
  "version": 2,
  "builds": [
    {
      "src": "PageNest/wsgi.py",
      "use": "@ardnt/vercel-python-wsgi",
      "config": {
        "maxLambdaSize": "45mb",
        "runtime": "python3.11" 
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "PageNest/wsgi.py"
    }
  ]
}