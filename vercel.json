{
  "version": 2,
  "builds": [
    {
      "src": "PageNest/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb" }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "PageNest/wsgi.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "PageNest.settings",
    "PYTHONUNBUFFERED": "1"
  }
}
